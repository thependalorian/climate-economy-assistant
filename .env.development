# Development Environment Configuration
# This file contains development-specific settings

# Enable development features
DEBUG=true
VITE_TRACE_ENABLED=true
VITE_VERBOSE=true
VITE_ENVIRONMENT=development

# Development API endpoints
VITE_API_BASE_URL=http://localhost:3000
VITE_DEV_MODE=true

# Development logging levels
VITE_LOG_LEVEL=debug
VITE_CONSOLE_TRACE=true

# Performance monitoring
VITE_PERFORMANCE_MONITORING=true
VITE_NETWORK_MONITORING=true

# Development tools
VITE_DEV_TOOLS=true
VITE_ERROR_OVERLAY=true

# Hot reload settings
VITE_HMR=true
VITE_HMR_PORT=24678

# Development database settings
VITE_DB_LOGGING=true
VITE_DB_QUERY_TRACE=true

# AI development settings
VITE_AI_DEBUG=true
VITE_AI_MOCK_RESPONSES=false

# Authentication development settings
VITE_AUTH_DEBUG=true
VITE_AUTH_BYPASS_EMAIL_VERIFICATION=true

# Development health checks
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_AUTO_HEALTH_CHECK=true

# Development notifications
VITE_DEV_NOTIFICATIONS=true
VITE_ERROR_NOTIFICATIONS=true

# Development analytics
VITE_ANALYTICS_DEBUG=true
VITE_TRACK_DEV_EVENTS=true
